#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ubuntu/repos/TuraAgent_Web3_New/tura-wallet/node_modules/.pnpm/autoprefixer@10.4.20_postcss@8.5.1/node_modules/autoprefixer/bin/node_modules:/home/ubuntu/repos/TuraAgent_Web3_New/tura-wallet/node_modules/.pnpm/autoprefixer@10.4.20_postcss@8.5.1/node_modules/autoprefixer/node_modules:/home/ubuntu/repos/TuraAgent_Web3_New/tura-wallet/node_modules/.pnpm/autoprefixer@10.4.20_postcss@8.5.1/node_modules:/home/ubuntu/repos/TuraAgent_Web3_New/tura-wallet/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/ubuntu/repos/TuraAgent_Web3_New/tura-wallet/node_modules/.pnpm/autoprefixer@10.4.20_postcss@8.5.1/node_modules/autoprefixer/bin/node_modules:/home/ubuntu/repos/TuraAgent_Web3_New/tura-wallet/node_modules/.pnpm/autoprefixer@10.4.20_postcss@8.5.1/node_modules/autoprefixer/node_modules:/home/ubuntu/repos/TuraAgent_Web3_New/tura-wallet/node_modules/.pnpm/autoprefixer@10.4.20_postcss@8.5.1/node_modules:/home/ubuntu/repos/TuraAgent_Web3_New/tura-wallet/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/autoprefixer" "$@"
else
  exec node  "$basedir/../../bin/autoprefixer" "$@"
fi
